<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:top="0dp"
        android:right="0dp"
        android:left="0dp">
        <shape
            android:shape="line">
            <solid android:color="@android:color/transparent"/>
            <stroke
                android:width="1dp"
                android:color="@color/colorGray"
                android:dashGap="5dp"
                android:dashWidth="4dp"/>
        </shape>
    </item>
</layer-list>